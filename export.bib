@article{Ellingham2018,
   abstract = {This study investigates the use of Scanning electron microscopy–energy-dispersive X-ray (SEM-EDX) as a diagnostic tool for the determination of the osseous origin of samples subjected to different temperatures. Sheep (Ovis aries) ribs of two experimental groups (fleshed and defleshed) were burned at temperatures of between 100°C and 1100°C in 100°C increments and subsequently analyzed with the SEM-EDX to determine the atomic percentage of present elements. Three-factor ANOVA analysis showed that neither the exposure temperature, nor whether the burning occurred with or without soft tissue present had any significant influence on the bone's overall elemental makeup (p > 0.05). The Ca/P ratio remained in the osseous typical range of between 1.6 and 2.58 in all analyzed samples. This demonstrates that even faced with high temperatures, the overall gross elemental content and atomic percentage of elements in bone remain stable, creating a unique “fingerprint” for osseous material, even after exposure to extreme conditions.},
   author = {Sarah T.D. Ellingham and Tim J.U. Thompson and Meez Islam},
   doi = {10.1111/1556-4029.13541;WGROUP:STRING:PUBLICATION},
   issn = {15564029},
