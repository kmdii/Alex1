   pmid = {28605021},
   publisher = {Blackwell Publishing Inc.},
   title = {Scanning Electron Microscopy–Energy-Dispersive X-Ray (SEM/EDX): A Rapid Diagnostic Tool to Aid the Identification of Burnt Bone and Contested Cremains},
